{
  "shared": {
    "compression_opts": 4,
    "decimals": 4,
    "filetype": "h5",
    "hierarchy_path":"{reeds_path}/inputs/hierarchy.csv",
    "output_timezone": "Etc/GMT+6", 
    "select_year": 2012
},

  "resource": {
    "bin_group_cols": ["region","class"],
    "bin_method": "equal_cap_cut",
    "class_bin_method": "kmeans",
    "copy_to_reeds": true,
    "copy_to_shared": false,
    "crs": "EPSG:5070",
    "driver": "H5FD_CORE",
    "dtype": "np.float16",
    "existing_sites": "{reeds_path}/inputs/capacity_exogenous/ReEDS_generator_database_final_EIA-NEMS.csv",
    "gather_method": "smart",
    "hourly_out_years": [2007,2008,2009,2010,2011,2012,2013,2016,2017,2018,2019,2020,2021,2022,2023],
    "inputfiles": ["reg_map_file", "class_path"],
    "offshore_meshed": false,
    "process_profiles": true,
    "profile_id_col": "sc_point_gid",
    "reg_map_file": "{reeds_path}/inputs/county2zone.csv",
    "rev_paths_file": "{reeds_path}/inputs/supply_curve/rev_paths.csv",
    "start_year": 2010,
    "state_abbrev": "{hourlize_path}/inputs/resource/state_abbrev.csv",
    "subsetvars": ["tech", "access_case"],
    "subtract_exog": false
  },

  "load": {
    "inputfiles": [],
    "legacy_ba_state_map_file": "{hourlize_path}/inputs/load/legacy_ba_state_map.csv",
    "legacy_ba_timezone_file": "{hourlize_path}/inputs/load/legacy_ba_timezone.csv",
    "load_source": "/projects/eerload/source_eer_load_profiles/20250512_eer_download/shape_outputs_2025-05-12",
    "load_source_timezone":"Etc/GMT+6",
    "model_years": [2021, 2025, 2030, 2035, 2040, 2045, 2050],
    "replace_sectors": [],
    "replace_states": "all",
    "replacement_share": {
      "2021": 1,
      "2025": 1,
      "2030": 1,
      "2035": 1,
      "2040": 1,
      "2045": 1,
      "2050": 1
    },
    "scenarios": ["IRA cons", "central", "baseline"],
    "sector_config_file": "{hourlize_path}/inputs/load/sector_config.json",
    "weather_years": [2007,2008,2009,2010,2011,2012,2013,2016,2017,2018,2019,2020,2021,2022,2023]
  }
  
}
